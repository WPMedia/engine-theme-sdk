{
  "name": "@wpmedia/engine-theme-sdk",
  "version": "2.13.3-arc-themes-release-version-2-0-3.6",
  "description": "SDK components for the Fusion News theme",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist",
    "types",
    "svg",
    "scripts"
  ],
  "scripts": {
    "build": "rm -rf dist && npm run build:cjs && npm run build:es && npm run build:dts",
    "build:cjs": "BABEL_ENV=commonjs babel src --extensions='.ts,.tsx' --out-dir dist/cjs --ignore 'src/components/**/*.test.tsx','src/components/**/*.test.ts' --no-copy-ignored",
    "build:es": "BABEL_ENV=es babel src --extensions='.ts,.tsx' --out-dir dist/es --ignore 'src/components/**/*.test.tsx','src/components/**/*.test.ts' --no-copy-ignored",
    "build:dts": "tsc",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "postpublish": "git push --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "node scripts/postinstall.js",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WPMedia/engine-theme-sdk.git"
  },
  "author": "",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/WPMedia/engine-theme-sdk/issues"
  },
  "homepage": "https://github.com/WPMedia/engine-theme-sdk#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addons": "^6.2.7",
    "@storybook/react": "^6.3.8",
    "@testing-library/react": "^12.1.2",
    "@types/babel__core": "^7.1.3",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.12.11",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chromatic": "^5.10.2",
    "enzyme": "^3.10.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@wpmedia/timezone": "^1.1.1",
    "dom-parser": "^0.1.6",
    "lazy-child": "^0.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "thumbor-lite": "^0.1.6"
  }
}
